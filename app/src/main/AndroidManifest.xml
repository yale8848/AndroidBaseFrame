        </activity>
        <activity android:name=".ui.test.recycwebview.SlideConfictActivity"
                  android:theme="@style/Theme.AppCompat.DayNight.NoActionBar">
        </activity>
        <activity android:name=".ui.test.recycwebview.WrapContentActivity"
