        <activity android:name=".ui.test.data.TestActivity"
                  android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name=".ui.test.brvah.ListActivity"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".ui.test.recycwebview.MainActivity"
